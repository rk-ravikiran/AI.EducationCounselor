# Google Cloud Authentication & Configuration
GOOGLE_APPLICATION_CREDENTIALS=C:\path\to\your\service_account.json
GOOGLE_PROJECT_ID=your-gcp-project-id
GOOGLE_LOCATION=us-central1

# Gemini API Key (fallback)
GOOGLE_API_KEY=your_api_key_here

# Corporate Proxy (if needed)
HTTP_PROXY=http://your.proxy.com:port
HTTPS_PROXY=http://your.proxy.com:port

# Model Configuration
MODEL_NAME=gemini-2.5-flash-lite
ALT_MODELS=gemini-2.5-flash-lite,gemini-1.5-flash-002,gemini-1.5-flash-001

# Features
DISABLE_VERTEX_EMBED=0
SUMMARY_LANG=en
LLM_DEBUG=1

# Web Search Configuration (Optional - for real-time course data)
GOOGLE_CUSTOM_SEARCH_API_KEY=your_custom_search_api_key
GOOGLE_CUSTOM_SEARCH_ENGINE_ID=your_search_engine_id
